    "concurrently": "^8.2.0",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "graphql": "^16.10.0"
  }
